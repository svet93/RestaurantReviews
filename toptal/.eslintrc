{
  "extends": "airbnb",
  "rules": {
    "prefer-destructuring": ["error", {"object": true, "array": false}],
    "camelcase":[0],
    "no-plusplus":"off",
    "no-unused-vars": ["error", { "args": "none" }],
    "global-require": [0],
    "no-underscore-dangle": [0],
    "no-restricted-globals": [0],
    "no-await-in-loop": [0],
    "no-nested-ternary": [0]
  }
}